# Interactive mode for file location managment
alias rmi='rm -i'
alias cpi='cp -i'
alias mvi='mv -i' 

# Navigation
alias -- -='cd -'
alias ..='cd ..'
alias ...='cd ../..'
alias ....='cd ../../..'
alias .....='cd ../../../..'

# Listing
alias l='ls -lart'
alias lsl='ls -l'
alias lsa='ls -la'

# Process utils
alias psgrep='ps -ef | grep'

# Utils
alias internetapps='lsof -P -i -n | cut -f 1 -d " "| uniq | tail -n +2'
