# Interactive mode for file location managment
alias rmi='rm -i'
alias cpi='cp -i'
alias mvi='mv -i' 

# Navigation made easy(er)
alias -- -='cd -'
alias ..='cd ..'
alias ...='cd ../..'
alias ....='cd ../../..'
alias .....='cd ../../../..'

# Listing
alias l='ls -lart'
alias lsl='ls -l'
alias lsa='ls -la'

# All Git alias are locatied in .git-aliases

# Process utils
alias psgrep='ps -ef | grep'

# Utils
alias internetapps='lsof -P -i -n | cut -f 1 -d " "| uniq | tail -n +2'
