#
# Interactive mode for file location managment
#
alias rmi='rm -i'
alias cpi='cp -i'
alias mvi='mv -i' 

#
# Navigation made easy(er)
#
alias -='cd -'
alias ..='cd ..'
alias ...='cd ../..'
alias ....='cd ../../..'
alias .....='cd ../../../..'

#
# All Git alias are locatied in .git-aliases
#

#
# Process utils
#
alias psgrep='ps -ef | grep'
